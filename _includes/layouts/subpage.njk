<!DOCTYPE html>
<html lang="{{ site.language }}">
<head>
    <title>{{ title }}</title>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  {% include 'partials/favicon.njk' %}

  <link rel="stylesheet" href="/css/responsive/max-width.css">
  <link rel="stylesheet" href="/css/responsive/responsive.css">
  <link rel="stylesheet" href="/css/responsive/container.css">
  <link rel="stylesheet" href="/css/responsive/buffer.css">
  <link rel="stylesheet" href="/css/typography.css">
  <link rel="stylesheet" href="/css/picture.css">
  <link rel="stylesheet" href="/css/spacers.css">
  <link rel="stylesheet" href="/css/utils.css">
  <link rel="stylesheet" href="/css/services.css">
  <link rel="stylesheet" href="/css/banner.css">
  <link rel="stylesheet" href="/css/button-2.css">
  <link rel="stylesheet" href="/css/accordion.css">
  <link rel="stylesheet" href="/css/accordion__control.css">

  <!-- Menu -->
  <link rel="stylesheet" href="/css/header.css">

  {% include 'partials/js-detect.njk' %}
</head>
<body>
  {% include 'partials/header.njk' %}
  {% max_width max_width %}
  {% responsive %}
  {% container containers, "100_100_100_45_45" %}
  <picture>
    <source media="(min-width: 1240px)" srcset="/img/bg-subpage-desktop.png">
    <source media="(min-width: 768px)" srcset="/img/bg-subpage-tablet.png">
    <source srcset="/img/bg-subpage-mobile.png">
    <img src="/img/bg-subpage-mobile.png" alt="" width="377" height="236">
  </picture>
  {% end_container %}
  {% container containers, "100_100_100_55_55" %}
  <div class="accordion-container">
  <div class="accordion-spacer-top"></div>
  <div id="accordion" class="accordion">
  {% for accordion in accordion__list %}
    {% accordion accordion.title, loop.index, accordion.text, accordion.list %}
  {% endfor %}
  </div>
  <div class="accordion-spacer-bottom"></div>
  </div>
  {% end_container %}
  {% end_responsive %}
  {% end_max_width %}

  {{ content | safe }}

  <!-- Header -->
  <script src="/js/utils.js"></script>
  <script src="/js/header.js"></script>
  <script src="/js/accordion.js"></script>
</body>
</html>